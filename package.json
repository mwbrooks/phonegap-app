{
  "name": "phonegap-app-developer",
  "description": "PhoneGap Developer app.",
  "homepage": "http://app.phonegap.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/phonegap/phonegap-app-developer.git"
  },
  "preferGlobal": "false",
  "private": "true",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdirp www/assets && ncp src/assets www/assets",
    "lint": "eslint --ignore-pattern src/assets {src,test}",
    "test": "NODE_ENV=production NODE_PATH=$NODE_PATH:$PWD/src mocha \"./src/**/*_test.js\" --compilers js:babel-core/register,css:css-null-compiler.js --recursive --require ./tests/setup.js",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "adobe-mobile-ui": "git+https://github.com/phonegap/adobe-mobile-ui.git",
    "history": "^4.2.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.0",
    "preact": "^7.0.3",
    "preact-compat": "^3.9.1",
    "preact-portal": "^1.1.1",
    "preact-redux": "^1.2.0",
    "preact-transition-group": "^1.1.0",
    "react-animated-views": "git+https://github.com/devgeeks/react-animated-views.git",
    "react-fastclick-alt": "^1.1.1",
    "react-intl": "^2.1.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "semver": "^5.3.0",
    "topcoat-preact": "git+https://github.com/phonegap/topcoat-preact.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "main": "src/index.js"
}
